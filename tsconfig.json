{
    "compilerOptions": {
        "target": "ES2023",
        "rootDir": "./src",
        "outDir": "./dist",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@Utils/*": ["./src/Utils/*"]
        }
    }
}
